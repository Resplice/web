{
	"useTabs": true,
	"tabWidth": 2,
	"semi": false,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 100,
	"svelteSortOrder": "scripts-markup-styles-options",
	"plugins": ["prettier-plugin-astro", "prettier-plugin-svelte"],
	"overrides": [
		{ "files": "*.astro", "options": { "parser": "astro" } },
		{ "files": "*.svelte", "options": { "parser": "svelte" } }
	]
}
